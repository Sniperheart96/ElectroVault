  "about": {
    "title": "About ElectroVault",
    "subtitle": "Community-maintained database for electronic components",
    "mission": {
      "title": "Our Mission",
      "description": "ElectroVault is a free and open database for electronic components - from historical tubes to modern nanotechnology. Our goal is to preserve knowledge about electronic components and make it accessible to everyone."
    },
    "features": {
      "title": "Features",
      "comprehensive": {
        "title": "Comprehensive Database",
        "description": "From resistors to capacitors to highly integrated circuits - we capture all important components with detailed specifications."
      },
      "historical": {
        "title": "Historical Archive",
        "description": "Documentation of historical components for repair and restoration of vintage equipment."
      },
      "community": {
        "title": "Community-driven",
        "description": "Everyone can contribute and share their knowledge. Together we build the most comprehensive component database."
      },
      "open": {
        "title": "Open Source",
        "description": "Fully transparent and freely available under MIT License."
      }
    },
    "contribute": {
      "title": "Get Involved",
      "description": "ElectroVault thrives on its community. Here are some ways you can contribute:",
      "dataEntry": "Add or edit components",
      "datasheets": "Upload datasheets",
      "code": "Code contributions on GitHub",
      "feedback": "Feedback and suggestions for improvement"
    }
  },
  "help": {
    "title": "Help & FAQ",
    "subtitle": "Frequently Asked Questions",
    "general": {
      "title": "General",
      "what": {
        "question": "What is ElectroVault?",
        "answer": "ElectroVault is a free, community-maintained database for electrical and electronic components. We document components from historical tubes to modern ICs with comprehensive metadata, datasheets, and specifications."
      },
      "free": {
        "question": "Is ElectroVault free?",
        "answer": "Yes, ElectroVault is completely free and open source. Both usage and source code are freely available."
      }
    },
    "search": {
      "title": "Search",
      "how": {
        "question": "How do I find a specific component?",
        "answer": "Use the search function in the top right or navigate through the categories. You can search by part number, manufacturer, or properties."
      },
      "filters": {
        "question": "What filters are available?",
        "answer": "You can filter by category, manufacturer, status (active/obsolete), package type, and many other technical parameters."
      }
    },
    "contribute": {
      "title": "Contributing",
      "account": {
        "question": "Do I need an account to contribute?",
        "answer": "Yes, to add or edit components, you need a free account. This helps us ensure data quality."
      },
      "how": {
        "question": "How can I add a component?",
        "answer": "After logging in, you can create new components via the 'New Component' button in the component overview. A moderator will review your contribution before publication."
      },
      "quality": {
        "question": "How is data quality ensured?",
        "answer": "All contributions are reviewed by experienced moderators. Additionally, registered users can suggest corrections."
      }
    }
  },
